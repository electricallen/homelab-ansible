{
  "storage": {
    "type": "b2",
    "config": {
      "bucket": "{{ kopia_bucket }}",
      "keyID": "{{ kopia_key_id }}",
      "key": "{{ kopia_key }}"
    }
  },
  "caching": {
    "cacheDirectory": "../cache",
    "maxCacheSize": 5242880000,
    "maxMetadataCacheSize": 5242880000,
    "maxListCacheDuration": 30
  },
  "hostname": "{{ kopia_host }}.{{ domain }}",
  "username": "{{ username }}",
  "description": "Docker Backups",
  "enableActions": false,
  "formatBlobCacheDuration": 900000000000
}